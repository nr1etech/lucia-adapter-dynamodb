  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.525.0",
    "@aws-sdk/util-dynamodb": "^3.525.0",
    "@lucia-auth/adapter-test": "^5.0.0",
    "@types/node": "^20.11.25",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "lucia": "^3.0.1",
    "shx": "^0.3.4",
    "testcontainers": "^10.9.0",
    "tsx": "^4.7.1",
